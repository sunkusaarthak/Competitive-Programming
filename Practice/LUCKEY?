#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int t;
	cin >> t;
	while(t--)
    {
        int f1, f2;
        string str;
        cin >> str;
        f1 = str[0]-'0';
        f1 += str[1]-'0';
        f1 += str[2]-'0';

        f2 = str[3]-'0';
        f2 += str[4]-'0';
        f2 += str[5]-'0';

        if(f1 == f2)
        {
            cout << "YES";
        }
        else
        {
            cout << "NO";
        }

        cout << "\n";
    }
    return(0);
}
