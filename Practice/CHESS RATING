#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    while(t--)
    {
        int a, b;
        cin >> a >> b;
        if(a==b)
        {
            cout << "0";
        }
        else
        {
            //cout << (b-a)/8.0;
            int c = ceil((b-a)/8.0);
            cout << c;
        }
        cout << "\n";
    }
}
